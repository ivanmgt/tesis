#para encontrar en que ruta esta un numero despues de un string
grep -rlP '"xtal_id":\s*51529' .
#para contar cuantos numeros despues de xtal_id hay
grep -rhoP '"xtal_id":\s*-?\d+(\.\d+)?' . | sed 's/.*: //' | wc -l
#hay 444784
#para contar la cantidad de numero unicos despues de un string
grep -rhoP '"xtal_id":\s*-?\d+(\.\d+)?' . | sed 's/.*: //' | sort -n | uniq | wc -
#son 74809, faltan 1039
#para contar cuantos numeros despues de xtal_id hay en un directorio
grep -oP '"eta":\s*-?\d+(\.\d+)?' metadata/metadata_minus_z_9.json | sed 's/.*: //' | wc -l
#para contar cuantas veces se repite un id en un directorio
 grep -rhoP '"xtal_id":\s*-?\d+(\.\d+)?' metadata/metadata_minus_z_9.json | sed 's/.*: //' | sort -n | uniq -c
#para mostrar que xtal_id se repiten por directorio
grep -rhoP '"xtal_id":\s*-?\d+(\.\d+)?' metadata/metadata_minus_z_9.json | sed 's/.*: //' | sort -n | uniq -d
#para saber que valores de eta hay
grep -rhoP '"eta":\s*-?\d+(\.\d+)?' . | sed 's/.*: //'
